(ns number-to-english-phrase.core-test
  (:require [clojure.test :refer :all]
            [number-to-english-phrase.core :refer :all]))

(deftest a-test
  (testing "FIXME, I fail."
    (is (= 0 1))))

(deftest parse-number-test
  (is (= "zero" (parse-number 0))))


(deftest addition-tests
  (is (= 5 (+ 3 2)))
  (is (= 10 (+ 5 5))))

