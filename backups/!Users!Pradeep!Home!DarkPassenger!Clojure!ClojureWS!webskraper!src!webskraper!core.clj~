(ns webskraper.core
  (:require
   [crouton.html :as html]
   [org.httpkit.client :as http]))

(defn foo
  "I don't do a whole lot."
  [x]
  (println x "Hello, World!"))

(def page (:body @(http/get "http://www.uswitch.com/broadband/packages/")))

(def html (html/parse-string page))

(identity html) ;; Identity is used to evaluate an expression in line when you cannot execute html 


